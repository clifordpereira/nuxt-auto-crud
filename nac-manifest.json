{
  "name": "nuxt-auto-crud",
  "version": "1.25.0",
  "type": "nuxt-module",
  "role": "core-engine",
  "description": "Zero-codegen SSOT CRUD engine for Nuxt 4 using Drizzle-Zod.",
  "related_repos": [
    {
      "name": "nuxt-auto-crud-template",
      "url": "https://github.com/clifordpereira/nuxt-auto-crud_template",
      "relationship": "reference-implementation",
      "purpose": "Showcases nuxt-auto-crud in a working environment"
    }
  ],
  "capabilities": {
    "api_generation": "dynamic-runtime",
    "validation": "drizzle-zod-inferred",
    "auth": "db-driven-rbac"
  },
  "runtime_context": {
    "logic_path": "src/runtime/server/api/_nac/[model]",
    "mapper": "src/runtime/server/utils/modelMapper.ts"
  },
  "agent_instructions": "./.ai-context.md"
}